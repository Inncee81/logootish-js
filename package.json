    "typedoc": "^0.15.2",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^15.0.2"
